{
    "$schema": "http://json-schema.org/draft/2019-09/schema#",
    "title": "Trigger",
    "description": "Event and target to trigger",
    "type": "object",
    "properties": {
        "event": {
            "type": "string",
            "description": "Name of entity event to fire"
        },
        "target": {
            "enum": ["self"],
            "description": "Entity Target"
        }
    }
}