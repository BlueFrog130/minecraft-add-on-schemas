{
    "$schema": "http://json-schema.org/draft/2019-09/schema#",
    "type": "object",
    "title": "Effect",
    "properties": {
        "effect": {
            "type": "string",
            "description": "Effect to add to this entity",
            "enum": ["speed","slowness","haste","mining_fatigue","strength","instant_health","instant_damage","jump_boost","nausea","regeneration","resistance","fire_resistance","water_breathing","invisibility","blindness","night_vision","hunger","weakness","poison","wither","health_boost","absorption","saturation","levitation","fatal_poison","conduit_power","slow_falling","bad_omen","village_hero"]
        },
        "duration": {
            "type": "number",
            "description": "duration in seconds"
        },
        "amplifier": {
            "type": "number",
            "description": "Amplifier level"
        },
        "ambient": {
            "type": "boolean",
            "description": "If it is to be considered an ambient effect\nBlueFrog's Note: Could not find example to verify type and purpose"
        },
        "visible": {
            "type": "boolean",
            "description": "Should the effect be considered visible"
        }
    }
}